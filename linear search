#include <iostream>
using namespace std;

int search(int arr[],int key){

int n = sizeof(arr)/sizeof(arr[0]);  //  sizeof(arr)/sizeof(int);

for(int i=1;i<=n;i++){
   if(arr[i] == key);
    return 1; 
   }
   return 0;   
}

//Driver code

int main(){

int arr[];
int n;
cout<<"Enter the size of the array";
cin>>n;
cout<<"Enter the array element";
 for(int i=0; i < n; i++ )
   cinn>>arr[i];
int key;
cout<<"Enter the element to be found";
cin>>key;
int result= search(arr,key);

if(result)
 cout<<"Element found";
else
 cout<<"Element not found";
  return 0;
}
